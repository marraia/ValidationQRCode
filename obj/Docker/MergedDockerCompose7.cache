services:
  montenegro.qrcode.validation:
    build:
      context: C:\_test\Montenegro.QrCode.Validation
      dockerfile: Montenegro.QrCode.Validation/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Montenegro.QrCode.Validation
      target: base
    container_name: Montenegro.QrCode.Validation
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: montenegroqrcodevalidation:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/Montenegro.QrCode.Validation.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5555
      target: 80
    tty: true
    volumes:
    - C:\_test\Montenegro.QrCode.Validation\Montenegro.QrCode.Validation:/app:rw
    - C:\Users\foes\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\foes\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\foes\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\foes\.nuget\packages:/root/.nuget/packages:ro
    - C:\_test\Montenegro.QrCode.Validation:/src:rw
version: '3.4'